<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 配置全局属性值 -->
	<settings>
		<!-- 使用JDBC的useGeneratedKeys 获取数据库自增主键值，默认为false -->
		<setting name="useGeneratedKeys" value="true"/>
		<!-- 使用列别名替换列名 默认为：true   select name as newName from table-->
		<setting name="userColumnLabel" value="true"/>
		<!-- 开启驼峰命名法转换，例：Tabel(create_time) -> Entity(createTime) -->
		<setting name="mapUnderscoreCamelCase" value="true"/>
		
	</settings>
</configuration>